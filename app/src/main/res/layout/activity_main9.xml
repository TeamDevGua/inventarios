<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity9">

    <TextView
        android:id="@+id/txtPrint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="179dp"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="179dp"
        android:text="Marbete"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtPrint2" />

    <EditText
        android:id="@+id/editText_Address_WiFi3"
        android:layout_width="147dp"
        android:layout_height="44dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="128dp"
        android:layout_marginBottom="204dp"
        android:background="?attr/colorAccent"
        android:ems="10"
        android:hint="IP Address"
        android:inputType="textPersonName"
        android:textSize="14sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtmbte" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="43dp"
        android:layout_height="42dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="111dp"
        android:background="?attr/colorAccent"
        android:onClick="onBackPressed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtmbte"
        app:srcCompat="?attr/homeAsUpIndicator" />

    <ImageButton
        android:id="@+id/imageButtons"
        android:layout_width="41dp"
        android:layout_height="42dp"
        android:layout_marginStart="115dp"
        android:layout_marginTop="111dp"
        android:layout_marginEnd="148dp"
        android:background="?attr/colorAccent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton2"
        app:layout_constraintTop_toBottomOf="@+id/txtmbte"
        app:srcCompat="@drawable/ic_action_print" />

    <EditText
        android:id="@+id/evcodmbt"
        android:layout_width="300dp"
        android:layout_height="42dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="56dp"
        android:background="?attr/actionModeBackground"
        android:digits="1234567890"
        android:ems="10"
        android:hint="escanea el codigo de marbete"
        android:inputType="number"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtPrint" />

    <TextView
        android:id="@+id/txtPrint2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="164dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="164dp"
        android:text="Re-ImpresiÃ³n"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtmbte"
        android:layout_width="300dp"
        android:layout_height="20dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="41dp"
        android:text="Esperando..."
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/evcodmbt" />
</androidx.constraintlayout.widget.ConstraintLayout>